{
  "name": "baseline-oracle",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "scan": "node packages/cli/dist/index.js scan --out .baseline/scan.json",
    "report": "node packages/cli/dist/index.js report .baseline/scan.json --html .baseline/report.html",
    "policy": "node packages/cli/dist/index.js policy .baseline/scan.json",
    "demo": "pnpm build && pnpm scan && pnpm report && start .baseline/report.html",
    "dev": "pnpm -r run dev"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "fast-glob": "^3.3.3",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "postcss-selector-parser": "^7.1.0",
    "web-features": "^3.1.0",
    "zod": "^4.1.11"
  }
}
